{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "my-chat-agent",
  "main": "src/server.ts",
  "compatibility_date": "2025-08-03",
  "compatibility_flags": ["nodejs_compat_v2"],
  "vars": {},
  "ai": {
    "binding": "AI"
  },
  "assets": {
    "directory": "public",
    "not_found_handling": "single-page-application",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": false,
    "logs": {
      "enabled": true,
      "persist": true
    }
  }
}
